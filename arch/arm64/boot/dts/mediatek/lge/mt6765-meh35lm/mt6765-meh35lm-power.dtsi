	POWER_ON_CAR_CHR = <(20)>;
	POWER_ON_CAR_NOCHR = <(-43)>;

	/*The ui_soc will keep 100% until SOC drop X percents after unplugged.*/
	KEEP_100_PERCENT = <(3)>;
	KEEP_100_PERCENT_MINSOC = <(9400)>;

#include "../battery_BL-T51.dtsi"
};

&odm {
	/* Battery ID */
	battery_id {
		compatible = "lge,battery-id";

		voltage-max = <4430000>;
		charge-full-design = <4000000>;
		model-name = "BLT51";

		ATL {
			id = "ATL";
			manufacturer = "ATL";
			type = "LGE_BLT51_ATL_4000mAh";
			profile-idx = <0>;
		};
		BYD {
			id = "BYD";
			manufacturer = "BYD";
			type = "LGE_BLT51_BYD_4000mAh";
			profile-idx = <1>;
		};
	};

	/* Cable ID */
	cable_id {
		compatible = "lge,cable-id";

		pinctrl-names = "default", "adc";
		pinctrl-0 = <&cable_id_default>;
		pinctrl-1 = <&cable_id_adc>;

		channel = <4>;
		delay = <50>;

		/*
		 * Pull-up : 1.8V 150k (1%)
		 * Pull-down : 680k (1%)
		 * ID : 56k (20%), 130k (20%), 910k (20%)
		 * ADC Accuracy : +-10mV
		 */
		range = <
		/*	min 	max 	type	*/
			393	549	6	/* 6: LT_CABLE_56K */
			676	855	7	/* 7: LT_CABLE_130K */
			1253	1347	11	/* 11: LT_CABLE_910K */
			1348	1500	8	/* 8: USB_CABLE_400MA */
		>;

		embedded-battery;
		debounce = <5>;
	};

	/* Charger Controller */
	charger_controller {
		compatible = "lge,charger-controller";

		/* base information */
		icl = <2000>;
		fcc = <2000>;
		vfloat = <4430>;
		vbus = <5000>;
		technology = <2>;
		batt_therm_comp = <4>;

		/* otp */
		otp-version = "2.1";
		otp-fcc = <1000>;
		otp-vfloat = <4000>;

		/* fb */
		fb-fcc = <1500>;

		/* spec */
		spec = <
			 0 15 4000 1900	/* 0.5C */
			 0 15 4450 1000	/* 0.3C */
			15 45 4250 3900	/* 1.0C */
			15 45 4450 2300	/* 0.6C */
			45 60 4100 1900	/* 0.5C */
		>;

		/* time to full */
		time_to_full_mode;
		battery_full_capacity = <4000>;
		sdp_current = <500>;
		dcp_current = <2000>;
		pep_current = <2200>;
		non_std_current = <500>;
		report_ttf_comp = <0>;
		sdp_comp = <3>;
		dcp_comp = <4>;
		pep_comp = <0>;
		pd_comp = <0>;
		cdp_comp = <4>;
		non_std_comp = <3>;
		min_comp = <0>;
			/*lgc tocad atl byd lishen*/
		batt_comp = <0 0 0 0 0>;
			/* current soc */
		cc_data = <2000 0>;
			/* current soc time */
		cv_data = <
			1892	811	2296
			1810	821	2227
			1733	831	2155
			1656	841	2083
			1576	851	2007
			1491	861	1923
			1413	871	1837
			1334	881	1748
			1253	891	1652
			1175	901	1551
			1095	911	1436
			1020	921	1318
			938	931	1189
			861	941	1051
			792	951	904
			718	961	743
			650	971	570
			588	981	377
			551	988	241
			519	991	144
			494	994	39
			481	996	0
			479	1000	0
		>;

		/* game */
		game-fcc = <700>;

		game-light-fcc = <700>;
		game-light-load = <80>;
		game-light-sec = <100>;

		game-lowbatt-fcc = <700>;
		game-lowbatt-soc = <15>;
	};

	/* Unified Power Sysfs */
	lge_power_sysfs {
		compatible = "lge,power-sysfs";
		sysfs,node =
			/* adc group */
			"adc", "batt_id", "/sys/class/power_supply/battery_id/valid_batt_id",
			"adc", "batt_therm", "/sys/class/power_supply/battery/temp",
			"adc", "thermal", "/sys/class/thermal/",
			"adc", "usb_id", "/sys/module/lge_cable_id/parameters/usb_id",
			"adc", "usb_in", "/sys/class/power_supply/charger/voltage_now",
			"adc", "vts", "/sys/class/thermal/thermal_zone24/temp",
			/* battery group */
			"battery", "battery_type", "/sys/class/power_supply/battery_id/battery_type",
			"battery", "capacity", "/sys/class/power_supply/battery/capacity",
			"battery", "cycle", "/sys/class/power_supply/battery/battery_cycle",
