&odm {
	/* Battery ID */
	battery_id {
		compatible = "lge,battery-id";

		voltage-max = <4430000>;
		charge-full-design = <4000000>;
		model-name = "BLT48";

		ATL_GEN2 {
			id = "ATL_GEN2";
			manufacturer = "ATL";
			type = "LGE_BLT48_ATL_4000mAh";
			profile-idx = <0>;
		};
		BYD_GEN2 {
			id = "BYD_GEN2";
			manufacturer = "BYD";
			type = "LGE_BLT48_BYD_4000mAh";
			profile-idx = <1>;
		};
	};

	/* Cable ID */
	cable_id {
		compatible = "lge,cable-id";

		pinctrl-names = "default", "adc";
		pinctrl-0 = <&cable_id_default>;
		pinctrl-1 = <&cable_id_adc>;

		channel = <4>;
		delay = <50>;

		/*
		 * Pull-up : 1.8V 150k (1%)
		 * Pull-down : 680k (1%)
		 * ID : 56k (20%), 130k (20%), 910k (20%)
		 * ADC Accuracy : +-10mV
		 */
		range = <
		/*	min 	max 	type	*/
			393	549	6	/* 6: LT_CABLE_56K */
			676	855	7	/* 7: LT_CABLE_130K */
			1253	1347	11	/* 11: LT_CABLE_910K */
			1348	1500	8	/* 8: USB_CABLE_400MA */
		>;

		embedded-battery;
		debounce = <5>;
	};

	/* Charger Controller */
	charger_controller {
		compatible = "lge,charger-controller";

		/* base information */
		icl = <2000>;
		fcc = <2200>;
		vfloat = <4430>;
		vbus = <9000>;
		chargerlogo-fcc = <2500>;
		technology = <2>;
		batt_therm_comp = <6>;

