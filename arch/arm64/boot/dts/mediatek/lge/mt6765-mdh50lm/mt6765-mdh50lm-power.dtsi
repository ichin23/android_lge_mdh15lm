		otp-version = "2.1";
		otp-fcc = <1100>;
		otp-vfloat = <4000>;

		/* fb */
		fb-fcc = <1400>;

		/* spec */
		spec = <
			 0 15 4000 1900	/* 0.5C */
			 0 15 4450  700	/* 0.3C */
			15 45 4250 5000	/* 1.3C */
			15 45 4450 2300	/* 0.6C */
			45 60 4100 1900	/* 0.5C */
		>;

		/* time to full */
		time_to_full_mode;
		battery_full_capacity = <4000>;
		sdp_current = <500>;
		dcp_current = <2000>;
		pep_current = <2200>;
		non_std_current = <500>;
		report_ttf_comp = <0>;
		sdp_comp = <(-2)>;
		dcp_comp = <2>;
		pep_comp = <2>;
		pd_comp = <2>;
		cdp_comp = <0>;
		non_std_comp = <(-2)>;
		min_comp = <0>;
			/*lgc tocad atl byd lishen*/
		batt_comp = <0 0 0 1 0>;
