&odm {
	/* Battery ID */
	battery_id {
		compatible = "lge,battery-id";

		LGC_GEN2 {
			id = "LGC_GEN2";
			voltage-max = <4430000>;
			charge-full-design = <3500000>;
			model-name = "BL-T44";
			manufacturer = "LGC";
			profile-idx = <0>;
		};
		TOCAD_GEN2 {
			id = "TOCAD_GEN2";
			voltage-max = <4430000>;
			charge-full-design = <3500000>;
			model-name = "BL-T44";
			manufacturer = "TOCAD";
			profile-idx = <1>;
		};
	};

	/* Cable ID */
	cable_id {
		compatible = "lge,cable-id";

		pinctrl-names = "default", "transition", "adc";
		pinctrl-0 = <&cable_id_default>;
		pinctrl-1 = <&cable_id_transition>;
		pinctrl-2 = <&cable_id_adc>;

		channel = <4>;
		delay = <50>;
		transition-delay = <50>;

		/*
		 * Pull-up : 1.8V 150k (1%)
		 * Pull-down : 680k (1%)
		 * ID : 56k (20%), 130k (20%), 910k (20%)
		 * ADC Accuracy : +-10mV
		 */
		otg-range = <0	200>;
		range = <
		/*	min 	max 	type	*/
			393	549	6	/* 6: LT_CABLE_56K */
			676	855	7	/* 7: LT_CABLE_130K */
			1253	1347	11	/* 11: LT_CABLE_910K */
			1348	1500	8	/* 8: USB_CABLE_400MA */
		>;

		embedded-battery;
		debounce = <5>;
	};

	/* Charger Controller */
	charger_controller {
		compatible = "lge,charger-controller";

		/* base information */
		icl = <1200>;
		fcc = <1300>;
		vfloat = <4430>;
		vbus = <5000>;
		technology = <2>;
		batt_therm_comp = <0>;

		/* otp */
		otp-version = "2.1";
		otp-fcc = <900>;
		otp-vfloat = <4000>;

		/* spec */
		spec = <
			 0 10 4000 1700	/* 0.5C */
			 0 10 4400  680	/* 0.2C */
			10 20 4000 1700	/* 0.5C */
			10 20 4450  680	/* 0.2C */
			20 45 4250 3400	/* 1.0C */
			20 45 4450 2040	/* 0.6C */
			45 60 4100 1700	/* 0.5C */
		>;

		/* time to full */
		time_to_full_mode;
		battery_full_capacity = <3500>;
		sdp_current = <500>;
		dcp_current = <1200>;
		pep_current = <2200>;
		non_std_current = <500>;
		report_ttf_comp = <0>;
		sdp_comp = <(-2)>;
		dcp_comp = <12>;
		pep_comp = <0>;
		pd_comp = <0>;
		cdp_comp = <0>;
		non_std_comp = <(-2)>;
		min_comp = <0>;
			/*lgc tocad atl byd lishen*/
		batt_comp = <(8) 0 0 0 0>;
			/* current soc */
		cc_data = <1200 0>;
			/* current soc time */
		cv_data = <
			1177 921 927
			1086 926 880
			1081 931 833
			1064 936 780
			1044 941 733
			1035 946 679
			1026 951 633
			940 956 579
			921 961 519
			873 966 458
			829 971 398
			766 977 311
			731 981 258
			670 988 157
			653 989 130
			642 991 105
			631 992 81
			617 993 54
			604 994 27
			586 995 0
			508 1000 0>;

		/* game */
		game-icl = <900>;

		game-light-icl = <900>;
		game-light-load = <80>;
		game-light-sec = <100>;

		game-lowbatt-icl = <900>;
		game-lowbatt-soc = <15>;
	};

	/* Unified Power Sysfs */
	lge_power_sysfs {
		compatible = "lge,power-sysfs";
		sysfs,node =
			/* adc group */
			"adc", "batt_id", "/sys/class/power_supply/battery_id/valid_batt_id",
			"adc", "batt_therm", "/sys/class/power_supply/battery/temp",
